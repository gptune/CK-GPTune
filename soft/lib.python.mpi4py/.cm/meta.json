{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "env_prefix": "CK_ENV_PYTHON_PACKAGE_MPI4PY",
    "limit_recursion_dir_search": {
      "linux": 5,
      "win": 5
    },
    "soft_file_universal": "mpi4py$#sep#$__init__.py",
    "version_module_name": "_version",
    "version_variable_name": "version_json"
  },
  "soft_name": "Python MPI4PY library",
  "tags": [
    "lib",
    "python-package",
    "mpi4py"
  ]
}
